<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.graduation.design.smartcampususerservice.dao.CampusDao">
    <insert id="addCampus" parameterType="Campus" keyProperty="id"
            useGeneratedKeys="true">
        INSERT INTO campus(campus_name,campus_code,province,official_system_id)VALUES(#{campusName},#{campusCode},#{province},#{officialSystemId})
    </insert>
</mapper>